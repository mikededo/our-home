{
  "name": "svelte-kit-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "conc \"bun dev:app\" \"bun check:watch\"",
    "dev:app": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint . --max-warnings=0",
    "lint:fix": "prettier --write . && eslint --fix .",
    "prepare": "husky && svelte-kit sync",
    "preview": "vite preview",
    "test": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "9.3.0",
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.80.0",
    "@sveltejs/adapter-auto": "3.2.1",
    "@sveltejs/adapter-vercel": "5.3.1",
    "@sveltejs/enhanced-img": "0.2.1",
    "@sveltejs/kit": "2.5.10",
    "@sveltejs/vite-plugin-svelte": "3.1.1",
    "@types/eslint": "8.56.10",
    "@types/node": "20.12.13",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "autoprefixer": "10.4.19",
    "concurrently": "8.2.2",
    "eslint": "9.3.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-svelte": "2.39.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.5",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.2.3",
    "prettier-plugin-tailwindcss": "0.5.14",
    "svelte": "5.0.0-next.144",
    "svelte-check": "3.8.0",
    "tailwindcss": "3.4.3",
    "tslib": "2.6.2",
    "typescript": "5.4.5",
    "typescript-eslint": "7.11.0",
    "vite": "5.2.12",
    "vitest": "1.6.0"
  },
  "type": "module",
  "dependencies": {
    "@supabase/ssr": "0.3.0",
    "@supabase/supabase-js": "2.43.4",
    "@tanstack/svelte-query": "5.40.0",
    "@tanstack/svelte-query-devtools": "5.40.0",
    "lucide-svelte": "0.350.0",
    "tailwind-merge": "2.3.0"
  }
}
